record(longin, "TPR:$(LOCA):$(IOC_UNIT):$(INST):PATT$(D)_PULSEID")
{
    field(DESC, "PatternDiag $(D) Pulse ID")
    field(DTYP, "asynInt32")
    field(INP,  "@asyn($(PORT),0) patternDiag$(I)_pulseId")
    field(SCAN, "I/O Intr")
}

